    "@bolt/components-button": "^0.9.8",
    "@bolt/components-card": "^0.9.6",
    "@bolt/components-critical-fonts": "^0.9.6",
    "@bolt/components-device-viewer": "^0.9.8",
    "@bolt/components-icon": "^0.9.8",
    "@bolt/components-image": "^0.9.8",
    "@bolt/components-nav-bar": "^0.9.8",
    "@bolt/components-smooth-scroll": "^0.9.5",
    "@bolt/components-sticky": "^0.9.0",
    "@bolt/components-video": "^0.9.6",
    "@bolt/core": "^0.9.6",
    "@bolt/elements-all": "^0.9.0",
    "@bolt/generic-all": "^0.9.0",
    "@bolt/objects-all": "^0.9.8",
    "@bolt/objects-ratio": "^0.9.8",
    "@bolt/settings-all": "^0.9.0",
