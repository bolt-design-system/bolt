{% include "@bolt-components-headline/headline.twig" with {
  size: "large",
  text: "Activate linked title by passing in an url"
} only %}

{% set navbar_data = [
  {
    text: "Link 1 Text",
    url: "#!"
  },
  {
    text: "Link 2 Text",
    url: "#!"
  },
  {
    text: "Link 3 Text",
    url: "#!"
  }
] %}

{% set navbar_content %}
  {% for item in navbar_data %}
    {% include "@bolt-components-navbar/navbar-item.twig" with item only %}  
  {% endfor %}
{% endset %}

{% include "@bolt-components-navbar/navbar.twig" with {
  title: {
    tag: "h2",
    text: "Title Text",
    icon: {
      name: "marketing-gray"
    },
    url: "#!"
  },
  content: navbar_content
} only %}
