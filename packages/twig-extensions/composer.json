{
  "name": "bolt/twig-extensions",
  "description": "Custom Twig Extensions Used By Bolt",
  "vendor": "bolt",
  "type": "patternlab-plugin",
  "authors": [
    {
      "name": "Salem Ghoweri",
      "role": "Maintainer"
    }
  ],
  "autoload": {
    "psr-0": {
      "Drupal\\Component\\Render\\": "/libs",
      "Drupal\\Component\\Utility\\": "/libs",
      "Drupal\\Core\\": "/libs",
      "Drupal\\Core\\Template\\": "/libs"
    }
  },
  "require": {
    "ksubileau/color-thief-php": "^1.3",
    "gregwar/image": "*",
    "mexitek/phpcolors": "^0.4.0",
    "symfony/yaml": "^3.3"
  },
  "extra": {
    "installer-name": "_twig-components"
  }
}
