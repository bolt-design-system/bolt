{% extends "@pl/default-template.twig" %}

{% block global_header %}
  {% include "@bolt-components-page-header/page-header.twig" %}
{% endblock %}

{% block main_content %}
  {% embed "@bolt-components-band/band--feature.twig" with {
    teaserPosition: "left",
    teaserWidth: "2/3",
    size: "large",
    full: true,
    theme: "dark",
    background: {
      opacity: "medium",
      fill: "gradient",
      focalPoint: {
        vertical: "center",
        horizontal: "right"
      },
      contentItems: [{
        pattern: "image",
        src: "/pattern-lab/images/placeholders/landscape-16x9-mountains.jpeg"
      }]


    },
    primaryTeaser: {
      headlines: [
        {
          type: "eyebrow",
          text: "Partner Overview",
          url: "pega.com",
          icon: {
            name: "arrow-left",
            position: "before"
          }
        },
        {
          type: "headline",
          text: "Find a Partner",
          size: "xxxlarge",
          tag: "h1"
        }
      ]
    }
  } only %}{% endembed %}

  <div class="c-bolt-band c-bolt-band--full c-bolt-band--small t-bolt-light">
    <div class="o-bolt-wrapper">
      <p class="c-bolt-text">
        Showing 555 results in
        <span class="c-bolt-search-inline-tags">
          <a href="javascript:" class="c-bolt-search-inline-tag">
            filter 1
          </a>
          <a href="javascript:" class="c-bolt-search-inline-tag">
            filter 2
          </a>
        </span>
        and <strong>"search term"</strong>
        (<a href="javascript:" class="c-bolt-link">Clear filters</a>)
      </p>
