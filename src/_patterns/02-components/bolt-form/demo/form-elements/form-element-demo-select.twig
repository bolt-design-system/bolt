{% extends "@bolt/form-element.twig" %}

{% set label_display = 'after' %}

{% set label %}
  {% include("@bolt/form-label.twig") with {
    "title": title,
    "displayType": "floating"
  } %}
{% endset %}

{% set children %}
  {% include("@bolt/form-select.twig") with {
    "attributes": selectAttributes,
    "options": options
  } %}
{% endset %}
