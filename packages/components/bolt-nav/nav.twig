<bolt-nav
  {% if headerOffset %} offset="{{ headerOffset }}" {% endif %}
>
  <ul class="c-bolt-nav {{ "c-bolt-nav--#{vspacing}" }} js-bolt-nav-gumshoe">
    {% for item in links %}
      <li class="c-bolt-nav__item">
        {% include "@bolt-components-navlink/navlink.twig" with item only %}
      </li>
    {% endfor %}
    <li class="c-bolt-nav__indicator js-bolt-nav-indicator"></li>
  </ul>
</bolt-nav>