{
  "name": "@bolt/bolt-starter",
  "description": "Drupal 8 Starter Theme using the Bolt Design System",
  "version": "2.4.0",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "FORCE_COLOR=1 npx npm-run-all --serial build:*",
    "build:en": "bolt build --prod",
    "build:ja": "bolt build --prod --config-file .boltrc--ja.js",
    "start": "bolt start",
    "test": "jest __tests__/index.js"
  },
  "dependencies": {
    "yargs": "^13.2.4",
    "jest": "^24.8.0",
    "patch-package": "^6.1.2",
    "postinstall-postinstall": "^2.0.0",
    "@bolt/build-tools": "latest",
    "@bolt/components-button": "^2.6.0-beta.1",
    "@bolt/components-headline": "^2.6.0-beta.1",
    "@bolt/components-icon": "^2.6.0-beta.1",
    "@bolt/components-link": "^2.6.0-beta.1",
    "@bolt/components-navbar": "^2.6.0-beta.1",
    "@bolt/components-sticky": "^2.6.0-beta.1",
    "@bolt/global": "^2.6.0-beta.1"
  }
}
